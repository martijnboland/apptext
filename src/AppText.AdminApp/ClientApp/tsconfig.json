{
  "compilerOptions": {
    "outDir": "../wwwroot/dist",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "moduleResolution": "node",
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "lib": [
      "es2016",
      "dom"
    ],
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}